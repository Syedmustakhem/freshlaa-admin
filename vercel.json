{
  "routes": [
    {
      "src": "^/(static|favicon.ico|manifest.json|icons|logo192.jpeg|logo512.jpeg)(/.*)?$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
